    "version": "0.2.0",
    "configurations": [
        {
            "name": "(vsdbg) Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "GST_PLUGIN_PATH",
                    "value": "${workspaceFolder}/out/vscode/P.uginWritersGuide/gst-plugin-tutorial/plugins/Debug"
                }
            ],
            "externalConsole": false,
        }
    ]
}